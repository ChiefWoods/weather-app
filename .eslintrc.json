{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "semi": "off",
      "space-before-function-paren": "off",
      "prefer-const": "warn",
      "quote-props": "off"
    }
}
